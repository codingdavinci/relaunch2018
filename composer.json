{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "bower-asset/fontawesome": "^5.10",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/block_exclude_pages": "^1.2",
        "drupal/blockgroup": "^1.1",
        "drupal/config_installer": "^1.8",
        "drupal/console": "~1.0",
        "drupal/core": "~8.7.0",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/field_group": "^3.0",
        "drupal/image_widget_crop": "^2.2",
        "drupal/insert": "^2.0",
        "drupal/matomo": "^1.7",
        "drupal/maxlength": "^1.0",
        "drupal/metatag": "^1.7",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/select2": "^1.1",
        "drupal/taxonomy_menu": "^3.3",
        "drupal/twig_tweak": "^2.1",
        "drupal/video_embed_field": "^2.0",
        "drupal/views_data_export": "^1.0",
        "drush/drush": "^9.0.0",
        "npm-asset/bootstrap": "^4.3.1",
        "npm-asset/hammerjs": "^2.0.8",
        "npm-asset/jquery-color": "^3.0.0",
        "npm-asset/jquery-countup": "^1.0",
        "npm-asset/jquery.easing": "^1.4.1",
        "npm-asset/popper.js": "^1.14.6",
        "npm-asset/select2": "^4.0",
        "npm-asset/typeface-barlow": "*",
        "npm-asset/waypoints": "^4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/config_installer": {
                "#2991182: Fix for RuntimeException during installation": "https://www.drupal.org/files/issues/2018-09-06/2991182-9.patch"
            },
            "drupal/better_exposed_filters": {
                "#2992717: Fix for toggling filter links": "https://www.drupal.org/files/issues/2018-08-15/multiple_links_filters-2992717-2.patch"
            }
        }
    }
}
