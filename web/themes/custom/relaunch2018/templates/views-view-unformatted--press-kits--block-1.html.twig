{#
/**
 * @see views-view.html.twig
 */
#}
<div class="container">
  {% if title %}
    <h3>{{ title }}</h3>
  {% endif %}
  <div class="card-columns">
    {% for row in rows %}
      {%
        set row_classes = [
          'card',
          default_row_class ? 'views-row',
        ]
      %}
      <div{{ row.attributes.addClass(row_classes) }}>
        {{- row.content -}}
      </div>
    {% endfor %}
  </div>
</div>
