{#
/**
 * @see views-view-list.html-twig
 */
#}
{% if attributes -%}
  <div{{ attributes.addClass('row') }}>
{% endif %}
  <div class="col-md-4 col-lg-4">
    {% if title %}
      <h2>{{ title }}</h2>
    {% endif %}
  </div>
  <div class="col-md-8 col-lg-8">
    <{{ list.type }}{{ list.attributes }}>

      {% for row in rows %}
        <li{{ row.attributes }}>
          {{- row.content -}}
        </li>
      {% endfor %}

    </{{ list.type }}>
  </div>

{% if attributes -%}
  </div>
{% endif %}
