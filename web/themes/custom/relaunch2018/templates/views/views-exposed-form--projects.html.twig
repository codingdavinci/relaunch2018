{#
/**
 * @see views-exposed-form.html.twig
 */
#}
{% if q is not empty %}
  {#
    This ensures that, if clean URLs are off, the 'q' is added first,
    as a hidden form element, so that it shows up first in the POST URL.
  #}
{{ q }}
{% endif %}
<div class="form--inline clearfix row">
  <div class="col-12">
    {{ form.project_type }}
  </div>
  <div class="col-12">
    {{ form.event }}
  </div>
  <div class="col-12">
    {{ form.field_event_target_id_views_entity_reference }}
  </div>
  <div class="col-12">
    {{ form.actions }}
  </div>
</div>
