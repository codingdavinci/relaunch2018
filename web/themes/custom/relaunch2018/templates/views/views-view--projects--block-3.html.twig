{#
/**
 * @see views-view.html.twig
#}
{%
  set classes = [
    'view',
    'view-' ~ id|clean_class,
    'view-id-' ~ id,
    'view-display-id-' ~ display_id,
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if title %}
    {{ title }}
  {% endif %}
  {{ title_suffix }}
  {% if header %}
    <div class="view-header row">
      <div class="col-md-6 col-lg-7">
        {{ header }}
      </div>
    </div>
  {% endif %}
  {% if more %}
    {{ more }}
  {% endif %}
  {% if exposed %}
    <div class="view-filters">
      {{ exposed }}
    </div>
  {% endif %}
  {% if attachment_before %}
    <div class="attachment attachment-before">
      {{ attachment_before }}
    </div>
  {% endif %}

  <div class="view-horizontal-navigation">
    <div class="horizontal-nav">
      <a href="#boxes-{{ uuid }}" class="horizontal-nav--left" role="button">
        <span class="horizontal-nav--prev--icon"></span>
      </a>
      <a href="#boxes-{{ uuid }}" class="horizontal-nav--right" role="button">
        <span class="horizontal-nav--next--icon"></span>
      </a>
    </div>
  </div>

  {% if rows %}
    <div class="view-content">
      {{ rows }}
    </div>
  {% elseif empty %}
    <div class="view-empty">
      {{ empty }}
    </div>
  {% endif %}

  {% if pager %}
    {{ pager }}
  {% endif %}
  {% if attachment_after %}
    <div class="attachment attachment-after">
      {{ attachment_after }}
    </div>
  {% endif %}
  {% if footer %}
    <div class="view-footer">
      {{ footer }}
    </div>
  {% endif %}
  {% if feed_icons %}
    <div class="feed-icons">
      {{ feed_icons }}
    </div>
  {% endif %}
</div>
