{#
/**
 * @see paragraph.html.twig
 */
#}
{%
  set classes = [
    'paragraph',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished'
  ]
%}
{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}
      {{ content.field_image }}
      <div class="jury--member--info--wrapper">
        {{ content.field_image }}
        <div class="jury--member--info">
          {{ content|without('field_image')|without('field_color') }}
        </div>
      </div>
    {% endblock %}
  </div>
{% endblock paragraph %}
