{#
/**
 * @see field.html.twig
 */
#}
{% apply spaceless %}
{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
  ]
%}
{% for item in items %}
  <div{{ attributes.addClass(classes, 'field__item') }}>&nbsp;
    {%- trans -%}
      (representing {{ item.content['#markup'] }} objects)
    {%- endtrans -%}
  </div>
{% endfor %}
{% endapply %}
