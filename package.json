    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.7",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.0",
    "merge-stream": "^1.0.1",
    "minimist": "^1.2.0",
    "node-sass-import-once": "^1.2.0",
    "path": "^0.12.7",
    "prettier": "^1.16.1",
